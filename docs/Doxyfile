PROJECT_NAME           	= "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER         	= @VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@


STRIP_FROM_PATH        	= @PROJECT_SOURCE_DIR@/lib
INPUT                  	= Markdown.md @PROJECT_SOURCE_DIR@/lib

FILE_PATTERNS          	= *.h \
                          *.cpp

RECURSIVE              	= YES


WARN_LOGFILE	       	= @CMAKE_CURRENT_BINARY_DIR@/doxygen.log
QUIET				   	= YES


USE_MDFILE_AS_MAINPAGE	= Markdown.md
